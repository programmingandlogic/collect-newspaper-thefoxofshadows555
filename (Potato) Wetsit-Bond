import stanford.karel.*;

public class CollectNewspaperKarel extends SuperKarel {
	public void run() {
		  moveToBeeper();
		  pickBeeper();
		  moveToCorner();
	 }
	//moves karel back to the starting position
		 private void moveToCorner() {
		  turnLeft();
		  turnLeft();
		  move();
		  move();
		  move();
		  turnRight();
		  move();
		  turnRight();
	 }
		 //moves karel to the "newspaper"
		 private void moveToBeeper() {
		  move();
		  move();
		  turnRight();
		  move();
		  turnLeft();
		  move();
	 }
		 

}
